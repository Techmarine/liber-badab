        "rules": [
          {
            "name": "Whispers of the Wind",
            "id": "05ac-73f5-907e-a5b6",
            "hidden": false,
            "description": "If Ahazra Redth is part of this army and on the battlefield the Mantis Warrior player gets +1 to the steal the initiative roll and Enemy reinforcement rolls are made a -1. In Addition, upon being placed on the Battlefield, Ahazra Redth can choose to have the Skirmish and/or the Light unit sub type."
          },
          {
            "name": "Chief Librarian",
            "id": "29cf-29a7-1e65-8900",
            "hidden": false,
            "description": "Azra Redth counts-as a Master of the Company for all rule interactions."
          },
          {
            "name": "Battle Psyker",
            "id": "0d85-3fcb-73f5-1679",
            "hidden": false,
            "description": "Ahazra may pick up to two of the following Psychic Disciplines; Biomancy, Divination, Pyromancy, Telekinesis, Telepathy, Thaumaturgy, well as knowing the Mirage spell."
          }
        ]
