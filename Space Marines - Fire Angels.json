        "rules": [
          {
            "name": "Master Gunner",
            "id": "657e-4102-8c97-e9dc",
            "hidden": false,
            "description": "While Tarnus Vale is transported in a non-flyer vehicle, one weapon may elect to use his Ballistic Skill when firing each shooting phase. If it does so, it also gains the Armourbane (Ranged) Special Rule. In addition, any unit he and his unit are transported within counts as having the Assault Transport special rule when he and his unit disembark in the controlling players movement phase."
          },
          {
            "name": "Vehicle Specialist",
            "id": "e7d7-1a11-2f21-4851",
            "hidden": false,
            "description": "If Tarnus Vale is taken, all of the following units within his detachment gain the Extra Armour Wargear free of charge: Rhino, Razorback, Predator, Vindicator, Whirlwind."
          },
          {
            "name": "No Retreat, No Surrender!",
            "id": "3455-2536-ab72-a6c3",
            "hidden": false,
            "description": "Tarnus Vale and any infantry unit with the Chapter Tactics (Fire Angels) Special Rule that he joins gains the Stubborn special rule."
          }
        ],
